#include<stdio.h>
int main()
{
char str[30];
printf("enter input:");
gets(str);
if(str[0]>='a'&& str[0]<='z') 
{
printf("the input is valid");
}
    if(str[0]>='0' && str[0]<='9')
    {
      printf("the input is valid");
    }
   
    if(str[0]>='A'&& str[0]<='Z')
    {
      printf("the input is invalid");
    }
       return 0;
}
