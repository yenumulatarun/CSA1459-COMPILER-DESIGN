%}
 
/Rule Section/
%%
\n lc++; //line counter
([ ])+ sc++; //space counter
\t tc++; //tab counter
ch++;     //characters counter
%%
int yywrap()
{
return 1;
}

 
int main()
{
    // The function that starts the analysis
    yylex();
     
    printf("\nNo. of lines=%d", lc); 
    printf("\nNo. of spaces=%d", sc);
    printf("\nNo. of tabs=%d", tc);
    printf("\nNo. of other characters=%d", ch);
     
}
